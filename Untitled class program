using System;

namespace Untitled class program
{
    class Program
    {
        static void Main(string[] args)
        {
            Doodle doodle1 = new Doodle

            doodle1.name = "Steve";
            doodle1.color = "green";

            doodle1.Moving();
            doodle1.Drawing();
        }
    }
}

class Doodle
{

    String name;
    String color;

    Void Moving
    {
        Console.WriteLine(name + "is moving")
    }

    Void Drawing
    {
        Console.WriteLine(name + "is drawing")
    }

}
